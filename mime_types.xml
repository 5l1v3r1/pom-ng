<?xml version="1.0" encoding="ISO-8859-1"?>

<mime_types>

	<definitions>
		<def name="javascript" description="Javascript" extension="js" class="application" />
		<def name="json" description="Javascript Object Notation" extension="json" class="application" />
		<def name="msword" description="Microsoft Word" extension="doc" class="document" />
		<def name="binary" description="Binary file" extension="bin" class="other" />
		<def name="pdf" description="Portable Document Format" extension="pdf" class="document" />
		<def name="excel" description="Excel Sheet" extension="xls" class="document" />
		<def name="rar" description="RAR file" extension="rar" class="application" />
		<def name="swf" description="Adobe Flash file" extension="swf" class="application" />
		<def name="form-urlencoded" description="URL encoded data" extension="txt" class="other" />
		<def name="jpeg" description="JPEG image" extension="jpg" class="image" />
		<def name="gif" description="GIF image" extension="gif" class="image" />

	</definitions>

	<types>
		<type def="javascript">application/javascript</type>
		<type def="json">application/json</type>
		<type def="msword">application/msword</type>
		<type def="binary">application/octet-stream</type>
		<type def="binary">application/octetstream</type>
		<type def="pdf">application/pdf</type>
		<type def="excel">application/vnd.ms-excel</type>
		<type def="powerpoint">application/vnd.ms-powerpoint</type>
		<type def="javascript">application/x-javascript</type>
		<type def="json">application/x-json</type>
		<type def="rar">application/x-rar-compressed</type>
		<type def="swf">application/x-shockwave-flash</type>
		<type def="form-urlencoded">application/x-www-form-urlencoded</type>
		<type def="jpeg">image/jpeg</type>
		<type def="jpeg">image/jpg</type>
		<type def="gif">image/gif</type>

	</types>

</mime_types>
